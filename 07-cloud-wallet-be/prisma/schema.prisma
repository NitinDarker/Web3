generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       String   @id @default(uuid())
  username String   @unique
  password String
  wallets  Wallet[]
}

model Wallet {
  id            String @id @default(uuid())
  publicKey     String
  encrypted_key String
  iv            String
  auth_tag      String
  userId        String
  user          User   @relation(fields: [userId], references: [id])
}